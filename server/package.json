    "pg": "^8.4.2",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/hapi__joi": "^17.1.2",
    "@types/jest": "^26.0.20",
    "@types/multer": "^1.4.3",
    "@types/supertest": "^2.0.10",
    "cors": "^2.8.5",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.2",
    "supertest": "^6.1.3",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.9.3"
  }
